body {
  background: #000000; /* Old browsers */
  /* IE9 SVG, needs conditional override of 'filter' to 'none' */
  background: url(data:image/svg+xml;base64,);
  background: -moz-radial-gradient(center, ellipse cover,  #000000 0%, #4a707a 1%, #4f6568 69%, #000000 100%); /* FF3.6+ */
  background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,#000000), color-stop(1%,#4a707a), color-stop(69%,#4f6568), color-stop(100%,#000000)); /* Chrome,Safari4+ */
  background: -webkit-radial-gradient(center, ellipse cover,  #000000 0%,#4a707a 1%,#4f6568 69%,#000000 100%); /* Chrome10+,Safari5.1+ */
  background: -o-radial-gradient(center, ellipse cover,  #000000 0%,#4a707a 1%,#4f6568 69%,#000000 100%); /* Opera 12+ */
  background: -ms-radial-gradient(center, ellipse cover,  #000000 0%,#4a707a 1%,#4f6568 69%,#000000 100%); /* IE10+ */
  background: radial-gradient(ellipse at center,  #000000 0%,#4a707a 1%,#4f6568 69%,#000000 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#000000', endColorstr='#000000',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */
  color: white;
  font-family: 'Scada', sans-serif;
}

#dashboardContent {
  background-color: #9E9E9E;
  height: 1000px;
  width: 90%;
  margin: auto;
  -webkit-border-radius: 10px 10px 10px 10px;
  border-radius: 10px 10px 10px 10px;
  text-align: center;
  padding: 20px;
}

#mainHeader {
  margin-top: -2%;
  height: 5%;
  font-family: 'Scada', sans-serif;
  text-align: center;
  margin-bottom: -1.5%;
}

#mainHeader a {
  font-size: 0.8em;
  letter-spacing: 0.4em;
  color: #F9F9F9;
  text-decoration: none;
}

#userDashboardTabs {
  height: 100%;
  overflow: auto;
  font-size: 0.9em;
}

#userDashboardTabs li:first-child {
  margin-left: 10%;
}

#userDashboardTabs li {
  font-size: 0.7em !important;
  margin: auto;
  margin-right: 6px;
}

